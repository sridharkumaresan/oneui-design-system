{
  "name": "@functions-oneui/react-utils",
  "version": "0.0.0",
  "description": "React-focused utilities shared by UI packages.",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "node ../../scripts/run-if-available.mjs tsc -p tsconfig.json",
    "lint": "node ../../scripts/run-if-available.mjs eslint . --max-warnings=0 --no-error-on-unmatched-pattern",
    "typecheck": "node ../../scripts/run-if-available.mjs tsc -p tsconfig.json --noEmit",
    "test": "echo \"No tests for @functions-oneui/react-utils yet\""
  },
  "dependencies": {"@functions-oneui/utils":"workspace:*"},
  "peerDependencies": {"react":"^18.0.0 || ^19.0.0","react-dom":"^18.0.0 || ^19.0.0"}
